{
  "name": "my-module",
  "type": "module",
  "version": "1.1.3",
  "packageManager": "pnpm@8.6.5",
  "description": "My new Nuxt module",
  "author": "Jamie Warburton <jamie@hexdigital.com>",
  "license": "MIT",
  "homepage": "https://github.com/hex-digital/nuxt-segment#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hex-digital/nuxt-segment.git"
  },
  "bugs": "https://github.com/hex-digital/nuxt-segment/issues",
  "keywords": [
    "nuxt",
    "nuxt3",
    "segment",
    "analytics"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.6.2",
    "@segment/analytics-core": "^1.3.0",
    "@segment/analytics-next": "^1.53.0",
    "defu": "^6.1.2",
    "sirv": "^2.0.3"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.6.2",
    "@nuxt/test-utils": "^3.6.2",
    "@types/node": "^18.16.19",
    "changelogen": "^0.5.4",
    "eslint": "^8.45.0",
    "nuxt": "^3.6.2",
    "vitest": "^0.33.0"
  }
}